{
	"name": "anime-infinity-x",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
		"ServerStorage": {
			"$className": "ServerStorage",
			"assets": {
				"$path": "assets/private"
			}
		},
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"server": {
				"$path": "out/server"
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					},
					"@flamework": {
						"$path": "node_modules/@flamework"
					}
				}
			},
			"shared": {
				"$path": "out/shared"
			},
			"assets": {
				"$path": "assets/public"
			}
		},
		"ReplicatedFirst":
		{
			"$className": "ReplicatedFirst",
			"setup": {
				"$path": "out/setup"
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"client": {
					"$path": "out/client"
				}
			}
		},
		"Workspace": {
			"$className": "Workspace",
			"$properties": {
				"FilteringEnabled": true
			}
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		},
		"TestService": {
			"$className": "TestService",
			"$properties": {
				"ExecuteWithStudioRun": true
			},
			"TS": {
				"$path": "scripts/testez/run-tests.server.lua"
			}
		}
	}
}
