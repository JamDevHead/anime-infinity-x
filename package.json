{
  "name": "anime-infinity-x",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "yarn eslint src && yarn prettier --check src",
    "lint:fix": "yarn eslint src --fix && yarn prettier --write src",
    "compile": "rbxtsc --verbose",
    "watch": "rbxtsc -w",
    "build:setup": "yarn compile && rojo build --output place-scripts.rbxlx",
    "build:prod": "cross-env NODE_ENV=production lune scripts/lune/build-project.luau --",
    "build:dev": "lune scripts/lune/build-project.luau -- --dev",
    "sync": "lune scripts/lune/sync-assets.luau",
    "sync:dev": "lune scripts/lune/sync-assets.luau -- --dev",
    "gen:types": "lune scripts/lune/generate-types.luau --",
    "tarmac": "tarmac sync --target roblox",
    "open:place": "scripts/open-place-binary",
    "serve": "rojo serve",
    "deploy:dev": "yarn build:dev && mantle deploy --environment canary",
    "deploy:prod": "yarn build:prod && mantle deploy --environment production",
    "output:dev": "mantle outputs --environment canary > mantle-output.json",
    "output:prod": "mantle outputs --environment production > mantle-output.json",
    "test": "lune scripts/lune/run-tests"
  },
  "devDependencies": {
    "@rbxts/compiler-types": "^2.2.0-types.0",
    "@rbxts/types": "^1.0.730",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-roblox-ts": "^0.0.36",
    "prettier": "^3.0.3",
    "rbxts-transform-env": "^2.2.0",
    "rbxts-transformer-flamework": "^1.0.0-beta.24",
    "roblox-ts": "^2.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@flamework/components": "^1.0.0-beta.24",
    "@flamework/core": "^1.0.0-beta.24",
    "@flamework/networking": "^1.0.0-beta.24",
    "@rbxts/abbreviate": "^3.0.4",
    "@rbxts/gizmo": "git+https://github.com/JamDevHead/gizmo.git",
    "@rbxts/log": "^0.6.3",
    "@rbxts/pretty-react-hooks": "^0.3.3",
    "@rbxts/profileservice": "^1.4.2",
    "@rbxts/react-reflex": "^0.2.0",
    "@rbxts/react-roblox": "^0.2.0",
    "@rbxts/reflex": "^4.2.2",
    "@rbxts/remo": "^1.2.4",
    "@rbxts/ripple": "^0.7.1",
    "@rbxts/roact": "npm:@rbxts/react-ts@latest",
    "@rbxts/services": "^1.5.1",
    "@rbxts/set-timeout": "^1.1.2",
    "@rbxts/t": "^3.1.0",
    "@rbxts/testez": "^0.4.2-ts.0",
    "@rbxts/trove": "^1.3.0",
    "@rbxts/zircon": "^1.0.9"
  }
}
