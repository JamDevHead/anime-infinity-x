{
  "name": "anime-infinity-x",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "yarn eslint src && yarn prettier --check src",
    "compile": "rbxtsc --verbose",
    "watch": "rbxtsc -w",
    "build:prod": "cross-env NODE_ENV=production yarn compile && rojo build --output place.rbxlx && yarn sync",
    "build:dev": "yarn compile && rojo build --output place.rbxlx && yarn sync:dev",
    "sync": "lune scripts/lune/sync-assets.luau",
    "sync:dev": "lune scripts/lune/sync-assets.luau dev",
    "serve": "rojo serve"
  },
  "devDependencies": {
    "@rbxts/compiler-types": "^2.2.0-types.0",
    "@rbxts/types": "^1.0.729",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-roblox-ts": "^0.0.36",
    "prettier": "^3.0.3",
    "rbxts-transform-env": "^2.2.0",
    "rbxts-transformer-flamework": "^1.0.0-beta.24",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@flamework/components": "^1.0.0-beta.24",
    "@flamework/core": "^1.0.0-beta.24",
    "@flamework/networking": "^1.0.0-beta.24",
    "@rbxts/log": "^0.6.3",
    "@rbxts/react-reflex": "^0.2.0",
    "@rbxts/react-roblox": "^0.2.0",
    "@rbxts/reflex": "^4.2.2",
    "@rbxts/roact": "npm:@rbxts/react-ts@latest",
    "@rbxts/services": "^1.5.1",
    "@rbxts/t": "^3.1.0",
    "@rbxts/zircon": "^1.0.9"
  }
}
